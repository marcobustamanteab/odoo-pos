<odoo>
    <record model="ir.ui.view" id="ccu_dte_client_account_journal_form_view">
        <field name="name">CCU DTE Client Account Journal</field>
        <field name="model">account.journal</field>
        <field name="priority">17</field>
        <field name="inherit_id" ref="account.view_account_journal_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='advanced_settings']" position="after">
                <page string="CCU Customization" name="ccu_customization">
                    <group>
                        <field name="dte_service_code"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>